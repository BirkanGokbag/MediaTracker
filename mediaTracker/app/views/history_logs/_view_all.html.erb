<table>
  <thead>
    <tr>
      <th>History Log</th>
    </tr>
  </thead>

  <tbody>
    <% @history_logs.each do |history_log| %>
      <tr>
        <td><%= history_log.users.username %> <%= history_log.historyMessage %> at <%= history_log.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
